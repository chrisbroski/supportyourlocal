<!doctype html>
<html lang="en">
<title>{{pageName}} - API - {{homeName}}</title>
{{{head}}}

<body id="site-page">
{{{header}}}

<article>
<h1>{{pageName}}</h1>

<h5><a href="{{API_DIR}}/">Home</a> &gt; {{pageName}}</h5>

{{#contactHtml}}
<section>
<h5>Contact Info</h5>
{{{.}}}
</section>
{{/contactHtml}}

<section>
{{{descHtml}}}

{{{bioHtml}}}

{{#hasSocialMedia}}
<h3>Social Media</h3>
{{#social.fb}}
<p>Facebook: <a href="{{.}}">{{.}}</a></p>
{{/social.fb}}
{{#social.spotify}}
<p>Spotify: <a href="{{.}}">{{.}}</a></p>
{{/social.spotify}}
{{#social.instagram}}
<p>Instagram: <a href="{{.}}">{{.}}</a></p>
{{/social.instagram}}
{{#social.youtube}}
<p>YouTube: <a href="{{.}}">{{.}}</a></p>
{{/social.youtube}}
{{#social.podcast}}
<p>Podcast: <a href="{{.}}">{{.}}</a></p>
{{/social.podcast}}
</section>
{{/hasSocialMedia}}

{{#hasPayment}}
<section>
<h3>Tip Jar</h3>
{{#payment.venmo}}
<p><a href="https://venmo.com/?txn=pay&audience=friends&recipients={{.}}">Venmo</a></p>
</section>
{{/payment.venmo}}
{{/hasPayment}}

{{#members}}
<section>
<h2>{{name}}</h2>
{{#photo}}
<img src="/photo/{{.}}" alt="{{name}}">
{{/photo}}
{{{location}}}
{{{bioHtml}}}
</section>
{{/members}}

</article>
